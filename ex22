    <script>
let a = Number(prompt("Digite o primeiro número:"));
let b = Number(prompt("Digite o segundo número:"));

if (a > b) {
  alert("O maior número é: " + a);
} else if (b > a) {
  alert("O maior número é: " + b);
} else {
  alert("Os números são iguais");
}
    </script>
    
