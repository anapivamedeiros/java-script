    <script>
let ano = Number(prompt("Digite um ano:"));
if ((ano % 400 === 0) || (ano % 4 === 0 && ano % 100 !== 0)) {
  alert(ano + " é um ano bissexto");
} else {
  alert(ano + " não é um ano bissexto");
}
    </script>
    
</body>
