    <script>
let senhaCorreta = "12345"; // Exemplo de senha correta
let senha = prompt("Digite a senha:");
if (senha === senhaCorreta) {
  alert("Senha correta. Acesso permitido.");
} else {
  alert("Senha incorreta. Acesso negado.");
}
    </script>
    
