    <script>
let x = Number(prompt("Digite o primeiro número:"));
let y = Number(prompt("Digite o segundo número:"));
let z = Number(prompt("Digite o terceiro número:"));

let maior = x;
if (y > maior) maior = y;
if (z > maior) maior = z;

alert("O maior número é: " + maior);os são iguais");
}
    </script>
    
