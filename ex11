    <script>
let valorDolar = Number(prompt("Valor em dólares (US$):"));
let cotacao = Number(prompt("Cotação do dólar (R$):"));

let valorReal = valorDolar * cotacao;
alert("Valor em reais: R$ " + valorReal.toFixed(2));
    </script>
    
